<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Vapolia.Mixpanel</id>
    <version>$version</version>
    <title>Mixpanel bindings for Xamarin iOS/Android</title>
    <authors>Benjamin Mayrargue, NinZine</authors>
    <owners>Benjamin Mayrargue</owners>
    <licenseUrl>http://www.vapolia.fr</licenseUrl>
    <projectUrl>https://github.com/softlion/mixpanel-xamarin</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Mixpanel bindings for Xamarin
    </description>
    <tags>mixpanel xamarin ios android</tags>
    <releaseNotes>
      Android Bindings 4.7 for android 4.0.3+
      iOS Bindings 2.9.1 armv7/arm64 only (simulator version i386 is no more available!)

      You need MonoAndroid51 compatible projects
    </releaseNotes>
    <dependencies>
      <group targetFramework="MonoAndroid51">
        <dependency id="Xamarin.Android.Support.v4" version="22.2.1.0"/>
        <dependency id="Xamarin.Android.Support.v7.AppCompat" version="22.2.1.0"/>
        <dependency id="Xamarin.GooglePlayServices.Base" version="25.0.0.0"/>
        <dependency id="Xamarin.GooglePlayServices.Gcm" version="25.0.0.0"/>
      </group>
    </dependencies>    
  </metadata>
  <files>

    <!-- droid -->
    <file src="..\MixpanelDroid\bin\Release\MixpanelDroid.dll" target="lib\MonoAndroid51\MixpanelAndroid.dll" />

    <!-- ios -->
    <file src="..\MixpanelBindings\bin\Release\MixpanelBindings.dll" target="lib\Xamarin.iOS\MixpanelBindings.dll" />

  </files>
</package>
